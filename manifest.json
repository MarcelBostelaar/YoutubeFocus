{
  "manifest_version": 2,
  "name": "YoutubeFocus",
  "version": "1.2",

  "description": "Makes youtube more focused by hiding all suggested content and comments.",

  "icons": {
    "96": "icons/96.png",
    "48": "icons/48.png"
  },

  "content_scripts": [
	{
      "matches": ["*://*.youtube.com/*"],
      "css": ["youtube_focus.css"]
	}
  ]
}
